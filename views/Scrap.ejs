                   <!-- display here -->
                   <div class=" p-1 d-flex flex-column">
                   
                    <%if(Scraps.length > 0){%>
                      <%Scraps.sort((a,b)=>{return b.createdAt - a.createdAt}).map(Scrap=>{%>
                      <!-- display here  -->
                      <span onclick="location.assign('/api/v1/scrap/single/<%=Scrap._id%>')" class="list-group position-relative mb-2">
                      <div class="list-group-item list-group-item-action d-flex justify-content-between align-items-center">
                        <small class="position-absolute top-0  start-50 translate-middle badge rounded-pill bg-warning">
                          <%if(Scrap.status==='Approved'){%>
                            <span class="badge bg-secondary">SCRAPED</span>
                            <%}else{%>
                             <%=Scrap.status%>
                             <%}%>
                       </small>
                        <a class="link-offset-2 link-offset-3-hover link-underline link-underline-opacity-0 link-underline-opacity-75-hover" href="/api/v1/scrap/single/<%=Scrap._id%>">
                               SCRP/<%=Scrap.ADC_CODE%>/<%=Scrap.SERIALnUMBER%>
                        </a>
                      </div>
                      </span>
                      <%})%>
                    <%}else{%>
                   <p>Nothig to display</p>
                    <%}%>
                    
                    </div>
                   
                   </div>
                </div>
                <div
                    class="tab-pane "
                    id="messages"
                    role="tabpanel"
                    aria-labelledby="messages-tab"
                >
                   <div class=" container border shadow p-4">
                       <!-- form space -->
                       <form  class="row g-3 needs-validation" id="form" >
                        <div class=" d-flex justify-content-between">
        
                           <h5> NEW SCRAP REQUEST</h5>
                           
                          </div>
          
                          <div class="col-md-4">
                            <label for="Manager" class="form-label">PRODUCT LIST</label>
                            <select name="storeProductId" id="Manager" class="form-select">
                                <option >Choose ....</option>
                                <%products.forEach((products)=>{%>
                                    <%pname = prud.filter(name=>{%>
                                        <% return name._id.toString() === products.productId.toString()%>
                                        <%})%>
                                         <%pname.map(n=>{%>
                                           <option value="<%=products.id%>"><%=n.Name.toUpperCase()%> [<%=n.ACDcode%>]</option>
                                         <%})%>
                                <%})%>
                            </select>
                          </div>
          
                        <div class="col-md-4">
                            <label for="customer" class="form-label">RAISED BY</label>
                            <input id="customer" class="form-control border" name="manager" value="<%=user.firstName%> <%=user.lastName%> " disabled>
                            <input type="text" value="<%=user._id%>"id="manager" name="manager" hidden disabled>
                            <input type="text" value="<%=result._id%>"name="WHID" hidden disabled>
                            <input type="text" value="<%=result.WHName%>"name="wareHouse" hidden disabled>
                          </div>
          
                        <div class="col-md-4">
                            <label for="date" class="form-label">PRODUCT CODE</label>
                            <input type="text" placeholder="PRODUCT CODE" required class="form-control border" id="date" name="ADC_CODE"/>
                        </div>
                        <div class="col-md-4">
                          <label for="SERIALnUMBER" class="form-label">PRODUCT SERIAL NUMBER</label>
                          <input type="text" placeholder="PRODUCT SERIAL NUMBER" required class="form-control border" autocomplete="off" id="SERIALnUMBER" name="SERIALnUMBER"/>
                      </div>
        
                      <div class="col-md-4">
                        <label for="REASON" class="form-label">SCRAP REASON</label>
                        <select name="REASON" required id="REASON" class="form-select">
                            <option value="">Choose...</option>
                            <option value="Damaged Package">Damaged Package</option>
                            <option value="Expired product">Expired product</option>
                            <option value="others">Others</option>
                        </select>
                    </div>
        
                    <div class="col-md-4">
                      <label for="files" class="form-label">ATTACHMENT </label>
                      <input type="file" placeholder="Bank Name" required class="form-control border" autocomplete="off" id="files"/>
                  </div>
                      <div class="col-md-4">
                        <label for="files" id="productImage" class="form-label">Preview </label>
                        <input type="url" name="image"  id="imageLoader" hidden>
                    </div>
                    
                       <div class="col-md-4">
                        <label for="remark" class="form-label">REMARKS</label>
                        <textarea type="text" id="remark"  name="remark"  class="form-control" autocomplete="off" autocapitalize="on" placeholder="Remark" ></textarea>
                      </div>
        
                        <div class="col-md-4  ">
                          <label for="InvoiceNo" class="form-label"> All done!</label>
                            <button class="btn btn-success form-control" > SUBMIT FOR APPROVAL</button>
                        </div>
          
                      </form>
                    
                </div>
               </div>
