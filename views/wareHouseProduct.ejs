                  width="150"height="150" >
                </ul>
                <!--ends here  -->
                  </td>
                  <td>
                    <!-- product name -->
                    <a href="/api/v1/Product/${product.productId}/" class="form-label">${product.productName.toUpperCase()}</a>
                  </td>
                  <td>
                    
                    <P>${product.category.toUpperCase()}</P>
                    <!-- ends here -->
                  </td>
                  <td>
                    <!-- product scale -->
                    <p>${product.currentQty}</p>
                  </td>
                  <td>
