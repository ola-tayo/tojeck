        <main class="pt-4 mt-5 mb-5">
            <!-- Tab panes -->
            <div class="tab-content mt-5 ">
           
                <div
                    class="tab-pane "
                    id="profile"
                    role="tabpanel"
                    aria-labelledby="profile-tab"
                >
                   <div class="container m-5 pb-5">
                   
                    <form  class="row g-3 mb-5 p-2 needs-validation border shadow bg-light" id="AccountForm">
                      <div class=" d-flex justify-content-between">
                        
                            <!-- <h5>Register Payment</h5> -->
                            <img width="60" height="60" src="data:image/png;base64,">
                          <h6><%=account.NAME.toUpperCase()%></h6>
                          
                        </div>
                        <p id="words" style="color:#ecb265" ></p>
                        <div class="col-md-4">
                            <label for="customer" class="form-label">Name</label>
                            <input type="text" name="NAME" id="" required readonly value="<%=account.NAME.toUpperCase()%>" class="form-control border" placeholder="Account Name">
                        </div>
          
                        <div class="col-md-4">
                          <label for="validationCustom01" class="form-label">Account type </label>
                          <input type="text" list="pickManager" value="<%= account.AccountType %>" name="AccountType" required class="form-control border" placeholder="Select Account Type" required/>
                          <datalist id="pickManager">
                              <option value="Assets">Assets</option>
                              <option value="Liabilities">Liabilities</option>
                              <option value="Equity">Equity</option>
                              <option valu="Expenses">Expenses</option>
                              <option value="Income">Income</option>
                          </datalist>
                      </div>
          
                      <div class="col-md-4">
                          <label for="validationCustom01" class="form-label">Account Number</label>
                          <input type="text"   class="form-control border" name="AccountNum" value="<%=account.AccountNum%>" readonly placeholder="Account Number"/>
                         
                      </div>
          
                      <div class="col-md-4">
                          <label for="validationCustom01" class="form-label">Current Balance</label>
                          <input type="number" readonly  class="form-control border" name="Balance" value="<%=account.Balance%>" placeholder="Opening Balance"/>
                      </div>
          
                      <div class="col-md-4">
                        <label for="locationOfUse" class="form-label">Location Of Use</label>
                       <select name="locationOfUse" id="locationOfUse" class="form-select" required>
                           <% if ( account.locationOfUse) { %>
                                <option class="bg-warning-subtle" selected value="<%= SelectedWhouse._id %>"><%= SelectedWhouse.WHName %> [Current]</option>
                        <% }else{%>
                            <option disabled value="">Choose...</option>
                        <%} %>
                        <%if(WHous.length > 0){%>
                          <%WHous.forEach((WHous)=>{%>
                            <option value="<%=WHous._id%>"><%=WHous.WHName%></option>
                            <%})%>
                          <%}%>
          
                       </select>
                    </div>
                    
                    <div class="col-md-4">
                      <label for="Initials" class="form-label">Bank Code</label>
                      <input name="Initials" id="Initials"value="<%= account.bankCode %>" readonly class="form-control" placeholder="Internal ref code" />
                  </div>
          
                      <div class="form-check">
                          <input class="form-check-input" type="checkbox" name="inactive" id="flexCheckDefault">
                          <label class="form-check-label" for="flexCheckDefault">
                            <%= account.inactive ? ' Activate Account':'Suspend usage' %>
                          </label>
                        </div>
    
                      <!-- <div class="col-md-4  ">
                        <label for="InvoiceNo" class="form-label"> All done!</label>
                          <button class="btn btn-success form-control" id="submit" >Update Account</button>
                      </div> -->
                      
                    </form>
          

                   </div>
                </div>
                <div
                    class="tab-pane active"
                    id="messages"
                    role="tabpanel"
                    aria-labelledby="messages-tab"
                >
                   <div  class="container-fluid ">
                       
                    <div
                        class="table-responsive container-fluid mt-3 "
                    >
                        <table
                            class="table table-dark "
                        >
                            <thead>
                                <tr>
                                    <th scope="col">Ref</th>
                                    <th scope="col">Posted By</th>
                                    <th scope="col">Payment Date</th>
                                    <th scope="col">Type</th>
                                    <th scope="col">Status</th>
                                    <th scope="col">CR</th>
                                    <th scope="col">DR</th>
                                    <th scope="col">Balance</th>
                                </tr>
                            </thead>
                            <tbody>
                                <!-- #endregion -->
                                <% if (entries.length > 0) { %>
                                 
                                    <% entries.forEach(element => { %>
                                     
                                        <tr class="">
                                            <td scope="row"><%= element.paymentReferenceNo %></td>
                                            <td scope="row"><%= element.Accountant %></td>
                                            <td scope="row"><%= element.PaymentDate %></td>
                                            <td scope="row"><%= element.collectionRef %></td>
                                            <td scope="row"><%= element.status %></td>
                                            <td scope="row">₦<%= element.crAmount ? element.crAmount : 0 %></td>
                                            <td scope="row">₦<%= element.drAmount ? element.drAmount : 0 %></td>
                                            <td scope="row">₦<%= element.previousBalance %></td>
                                        </tr>

                                    <% }) %>

                                <% } %>
                                
                            </tbody>
                        </table>
                    </div>
                   </div>
                    
                </div>
               </div>
        </main>
        <footer style="z-index: 3 !important;">
