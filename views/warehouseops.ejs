           <!-- Nav tabs -->
          
        </header>

        <main class="pt-4 ">
            <!-- Tab panes -->
            <div class="tab-content mt-5 ">
           
                <div
                    class="tab-pane active"
                    id="profile"
                    role="tabpanel"
                    aria-labelledby="profile-tab"
                >
                   <div class="container-fluid ">
                   <!-- display here -->
                   <div class="h-59 p-1 d-flex table-responsive custom-table-responsive animate__animated animate__backInLeft"  style="overflow-y: hidden ;">
                    <%if(results.DeliveredBill.length > 0){%>
                      <table class="border w-100">
                        <thead class="border">
                          <td class="border"><b>REF NO</b></td>
                          <td class="border"><b>CUSTOMER</b></td>
                          <td class="border"><b>ORDER DATE</b></td>
                          <td class="border"><b>DELIVERY FEE (₦)</b></td>
                          <td class="border"><b>INVOICE DATE</b></td>
                          <td class="border"><b>WayBill No</b></td>
                          <td class="border"><b>DISPATCHER</b></td>
                          <td class="border"><b>STATUS</b></td>
                          <td class="'border"><b>VALUE</b></td>
                        </thead>
                        <tbody id="tbody" class="mb-5">
                          <%results.DeliveredBill.sort((a,b)=>{return b.createdAt - a.createdAt}).forEach((Delivery)=>{%>
                          <!-- /invoice/<%=Delivery._id%> "/Delivery/Excel/<%=Delivery._id%>" -->
                          <tr class="border" title="<%=Delivery.customerName.toUpperCase()%> - <%=Delivery.billStatus.toUpperCase().substring(0,3)%>/<%=Delivery.billReferenceNo%>/<%=Delivery.invoicelocation%>">
                            <td class=""><a 
                              <%if(Delivery.billStatus === 'Cancelled'){%>
                                class="text-danger"
                               <%}%>"
                              href="/api/v1/bill/<%=Delivery._id%>"><b><%=Delivery.billStatus.toUpperCase().substring(0,3)%>/<%=Delivery.billReferenceNo%>/<%=Delivery.invoicelocation%></b>
                            </a></td>
                            <td class=""><%=Delivery.customerName.toUpperCase()%></td>
                            <td class=""><%=Delivery.startDate%></td>
                            <td class="">₦<%=Delivery.shippingFee%></td>
                            <td class=""><%=Delivery.DELIVERYDATE ? Delivery.DELIVERYDATE.toString().substring(0,15) : ''%></td>
                            <td class=""><%=Delivery.WayBillNo ?Delivery.WayBillNo : ''%></td>
                            <td class=""><%=Delivery.DriverName.toUpperCase()%></td>
                            <td class=" p-1 text-center"><p class=" shadow rounded text-light 
                              <%if(Delivery.billStatus === 'Cancelled'){%>
                                bg-danger
                               <%}else if(Delivery.billStatus.toLowerCase() === 'INVOICE'.toLowerCase()){%>
                                bg-success
                               <%}else{%>
                                bg-secondary
                               <%}%>
                              "><%=Delivery.billStatus.toUpperCase()%></p></td>
                            <td class="border">₦<%=Delivery.grandTotal%></td>
                          </tr>
                          <%})%>
                        </tbody>
                    </table>
                    <%}else{%>
                      <p>No Delivery Done yet</p>
                      <%}%>
                    </div>
                   
                   </div>
                </div>
                <div
                    class="tab-pane"
                    id="messages"
                    role="tabpanel"
                    aria-labelledby="messages-tab"
                >
                   <div class="container mb-5 pb-5">
                       <!-- delivery -->

                       <form  class="row g-3 needs-validation border shadow mt-2 mb-5 p-3 bg-light-subtle animate__animated animate__backInLeft" id="form">
                        <div class=" d-flex justify-content-between">
                          
                          
                          <h5> GENERATE WAY BILL</h5>
                          
                         <img src="data:image/png;base64,"
                         width="40"
                         height="40"
                         >
                          </div>
          
                          <div class="col-md-4">
                            <label for="billreferenceNo" class="form-label">SELECT INVOICE  REF NUMBER FOR PICK UP </label>
                            <select id="billreferenceNo" name="referenceNo"  required class="form-select border" >
                              <!-- display options for Bills -->
                              <option >Choose...</option>
                              <%if(Bills.length > 0){%>
                                 <%Bills.forEach(bill =>{%>
                                     <option value="<%=bill._id%>"><%=bill.billReferenceNo%></option>
                                 <%})%>
                               <%}%>
                            </select>
                        </div>
          
                        <div class="col-md-4">
                            <label for="customer" class="form-label"> BILLED FOR </label>
                            <h5 id="customer"></h5>
                          </div>
                          
                          <div class="col-md-4">
                            <label for="Grandtotals" class="form-label"> BILL TOTAL (₦)</label>
                            <h5 id="Grandtotals"> </h5>
                        </div>
          
                        <div class="col-md-4">
                            <label for="date" class="form-label">DELIVERY DATE</label>
                            <input type="date" placeholder="" required class="form-control border" id="date" name="date"/>
                        </div>
                        <div class="col-md-4">
                          <label for="DriverName" class="form-label">DRIVER NAME</label>
                          <input type="text" placeholder="" required class="form-control border" id="DriverName" name="DriverName"/>
                      </div>
                      <div class="col-md-4">
                        <label for="DriverNumber" class="form-label">DRIVER NUMBER</label>
                        <input type="text" placeholder="" required class="form-control border" id="DriverNumber" name="DriverNumber"/>
                    </div>
                    <div class="col-md-4">
                      <label for="fee" class="form-label">DELIVERY FEE (₦)</label>
                      <input type="number" placeholder="" required class="form-control border" id="fee" name="fee"/>
                    </div>
                    <div class="col-md-4">
                      <label for="plateNumber" class="form-label">DISPATCHER PLATE NUMBER</label>
                      <input type="text" placeholder="Eg ABC-324-XY" required class="form-control border" id="plateNumber" name="plateNumber"/>
                  </div>
                        <h5>Dispatcher Order</h5>
                       <!-- table sowing products here -->
                       <table class="border">
                        <thead class="border">
                          <td class="border">S/N</td>
                          <td class="border">Product</td>
                          <td class="border">CRTN</td>
                          <td class="border">ROLL</td>
                        </thead>
                        <tbody id="orders" ></tbody>
                       </table>
          
                       <div class="col-md-4">
                        <label for="remark" class="form-label">DELIVERY REMARKS</label>
                        <textarea type="text" id="remark"  name="remark"  class="form-control" autocomplete="off" autocapitalize="on"  ></textarea>
                      </div>
        
                        <div class="col-md-4  ">
                          <label for="InvoiceNo" class="form-label"> All done!</label>
                            <button class="btn btn-success form-control" id="submit" >SAVE WAYBILL</button>
                        </div>
          
                      </form>
                       
                   </div>
                    
                </div>
               </div>
        </main>
        <footer style="z-index: 3 !important;">
            <!-- place footer here -->
            <div class="container-fluid fixed-bottom bg-dark-subtle ">
                <div class="row p-2">
                    <!-- for next and filter -->
                    <div class="contaner d-flex justify-content-between">
                      <div>
                               <!-- next and prev btn -->
                       <button class="btn me-2 btn-dark"  <%if(!results.Previous){%>
                         disabled
                         <%}%>
                         <%if(results.Previous){%>
                           class="btn btn-dark shadow-sm me-2"
                           onclick="location.assign('/api/v1/Location/<%=result._id%>/?page=<%=results.Previous.page%>&limit=14&category=<%=results.currentCat%>')"
                         <%}%>
                         >prev
                         </button>
                         <button 
                         class="btn me-2 btn-dark"
                         <%if(!results.next){%>
                           disabled
                           <%}%>
                           <%if(results.next){%>
                             class="btn btn-dark shadow-sm me-2"
                             onclick="location.assign('/api/v1/Location/<%=result._id%>/?page=<%=results.next.page%>&limit=14&category=<%=results.currentCat%>')"
                         <%}%>
                         >Next</button>
                       <!--  -->
                      </div>
   
                      <div>
                          <!-- filre category -->
                       <%if(results.DeliveredBill.length > 0){%>
                        Group By: 
                         <div class="form-check form-check-inline">
                           <input
                             class="form-check-input"
                             type="radio"
                             name="category"
                             id="all"
                             value="all"
                             <%if(results.currentCat === 'all'){%>
                               checked="checked"
                             <%}%>
                             onchange="location.assign(`/api/v1/Location/<%=result._id%>/?page=<%=results.currentPage%>&limit=14&category=${this.value}`)"
                           />
                           <label class="form-check-label" for="all">ALL</label>
                         </div>
                         <div class="form-check form-check-inline">
                           <input
                             class="form-check-input"
                             type="radio"
                             name="category"
                             id="Order"
                             value="Order"
                             <%if(results.currentCat === 'Order'){%>
                               checked="checked"
                             <%}%>
                             onchange="location.assign(`/api/v1/Location/<%=result._id%>/?page=<%=results.currentPage%>&limit=14&category=${this.value}`)"
                           />
                           <label class="form-check-label" for="Order">Sales Order</label>
                         </div>
                         <div class="form-check form-check-inline">
                           <input
                             class="form-check-input"
                             type="radio"
                             name="category"
                             value="Invoice"
                             id="Invoice"
                             <%if(results.currentCat === 'Invoice'){%>
                               checked="checked"
                             <%}%>
