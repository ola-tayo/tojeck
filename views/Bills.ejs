<!DOCTYPE html>
<html lang="en">
    <%-include('./partials/head.ejs')%>
    <%-include('./partials/inactive.ejs')%>
    <style>
      .position-relative:hover{
          background-color: rgba(169, 169, 169, 0.568);
          color:rgb(216, 175, 42)
      }
      .position-relative{
          color:black
      }
  </style>
<body>
  <nav class="navbar bg-black">
    <div class="container-fluid">
        <a class="navbar-brand" href="/api/v1/Dashboard/<%=user._id%>" title="Apps">
            <img src="data:image/png;base64,
            
            
            
            " width="30" height="30">
          </a>
          <ul class="navbar-nav me-auto mb-2 mb-lg-0">
            <li class="nav-item dropdown">

                <h5>
                  <i style="color:<%if(result.Status){%>
                    #fff
                    <%}else{%>
                        red
                        <%}%>">
                      <b><%=result.WHName.toUpperCase()%></b>
                  </i>
                </h5>
                
              </li>
        </ul>
        <%-include('./partials/USER.ejs')%>
    </div>
  </nav>
   <div class="m-3 flex-col">
       <%if(result.Status){%>
        <div class="d-flex justify-content-around p-3 ">
          <%if(user._id.toString() === result.StoreKeeper._id.toString() || user.role === 'Admin' ||  user._id.toString() === result.Manager._id.toString()){%>
          <span onclick="location.assign('/api/v1/warehouse/purchase/<%=result._id%>')" title="purchase Request">
             <img height="40" width="40" src="data:image/png;base64,">
          </span>
          <%}%>
          <%if(user.jobTittle === 'Invoicer' || user.role === 'Admin' || user._id.toString() === result.Manager._id.toString()){%>
          <span onclick="location.assign('/api/v1/warehouse/<%=result._id%>/Invoices/new')" title="Generate Quotation">
             <img height="40" width="40" src="data:image/png;base64,">
          </span>
          <%}%>
          <%if(user._id.toString() === result.StoreKeeper._id.toString() || user.role === 'Admin' || user._id.toString() ===  result.Manager._id.toString()){%>
          <span onclick="location.assign('/api/v1/Replenish/<%=result._id%>/storeproduct')" title="Replenish Product">
             <img height="40" width="40" src="data:image/png;base64,">
          </span>
          <%}%>
          <span onclick="location.assign('/api/v1/Staff/<%=result._id%>')" title="Ware House Staff">
             <img height="40" width="40" src='https://cdn.pixabay.com/photo/2019/08/11/18/59/icon-4399701_640.png'>
          </span>
     </div>
        <%}%>
        <div class="d-flex border justify-content-between align-items-center px-2">
           
          <i style="color:<%if(result.Status){%>
            green
            <%}else{%>
                red
                <%}%>
            ">
            <img src="data:image/png;base64,"
            width="40"
            height="40"
            >
           </i> 
          
            <div class="d-flex align-items-start">
              <span  onclick="location.assign('/api/v1/warehouse/Product/<%=result._id%>')"  class=" p-2 font-bold position-relative" title="Product in Storage">
                <img src="data:image/png;base64,"
                width="40"
                height="40"
                >
             </span>
              <span onclick="location.assign('/api/v1/Location/<%=result._id%>')" rel="noopener noreferrer" class=" p-2 font-bold position-relative" title="To-Recieve" >
                 <img src="data:image/png;base64,"
                 width="40"
                 height="40"
                 >
                </span>
             <a href="/api/v1/Notification/<%=result._id%>"class=" p-2 font-bold position-relative" title="Notification">
                <span class="position-absolute top-0 start-100 translate-middle badge rounded-pill bg-danger shadow">
                    <%=result.Notification.length%>
                  </span>
                  <img src="data:image/png;base64,"
                  width="40"
                 height="40"
                  >
             </a>
             <a  href="/api/v1/LPO/<%=result._id%>" class=" p-2 font-bold position-relative" title="LPO">
              <img width="40" height="40" src="https://img.icons8.com/fluency/48/shopping-bag.png" alt="shopping-bag"/>
            </a>
            <a  href="/api/v1/Transfers/<%=result.WHName%>/<%=result._id%>" class=" p-2 font-bold position-relative" title="Inter Ware-house Transfers">
              <img width="40" height="40" src="https://img.icons8.com/stickers/100/back-sorting.png" alt="back-sorting"/>
            </a>
             <a  href="/api/v1/Scrap/<%=result._id%>"class=" p-2 font-bold position-relative" title="Scrap">
                 <img src="data:image/png;base64,"
                 width="40"
                 height="40"
                 >
             </a>
             <a  href="/api/v1/PR/<%=result._id%>" class=" p-2 font-bold position-relative" title="P/R Log">
              <img width="40" height="40" src="https://img.icons8.com/fluency/48/procurement.png" alt="procurement"/>
            </a>
             <a  href="/api/v1/Expense/<%=result._id%>"  class=" p-2 font-bold position-relative" title="Expense">
                 <img src="data:image/png;base64,"
                 width="40"
                 height="40"
                 >
             </a>
             <a  href="/api/v1/Returns/<%=result._id%>"  class=" p-2 font-bold position-relative" title="Purchase Returns" >
              <img  width="40"
              height="40"
            src="data:image/png;base64,">
            </a>
             <span  onclick="location.assign('/api/v1/warehouse/<%=result._id%>/Bills')"  class=" p-2 font-bold position-relative"" title="Invoice / quotations" style="background-color:#fff;box-shadow: inset 1px 2px 4px 1px silver;">
                <span class="position-absolute top-0 start-100 translate-middle badge rounded-pill bg-success shadow">
                  
                  </span>
                  <img src="data:image/png;base64,"
                  width="40"
                  height="40"
                  >
                </span>
            </div>
        </div>

        <div class="accordion" id="accordionExample">
          <div class="accordion-item">
            <h2 class="accordion-header">
              <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                <strong>Infomation</strong>
              </button>
            </h2>
            <div id="collapseOne" class="accordion-collapse collapse " data-bs-parent="#accordionExample">
              <div class="accordion-body row g-3 needs-validation">
                  
                  <div class="col-md-4">
                      <label for="validationCustom01" class="form-label">Manager</label>
                      <h5 id="validationCustom01" ><%=result.Manager.firstName.toUpperCase()%> <%=result.Manager.lastName.toUpperCase()%></h5>
                    </div>
                    <div class="col-md-3">
                      <label for="validationCustom02" class="form-label"> Email</label>
                      <div class="input-group has-validation">
                          <a  href="mailto:<%=result.Email%>" class="input-group-text" id="inputGroupPrepend">@</a>
                           <a href="mailto:<%=result.Email%>" class="form-control"  id="validationCustomUsername" aria-describedby="inputGroupPrepend"><%=result.Email%></a> 
                        </div>
                      
                    </div>
                    <div class="col-md-3">
                      <label for="validationCustomUsername" class="form-label">Telephone</label>
                      <div class="input-group has-validation">
                        <a href="tel:+234<%=result.Tel%>" class="input-group-text" id="inputGroupPrepend1">&phone;<a>
                        <a  href="tel:+234<%=result.Tel%>" class="form-control" id="validationCustomUsername" aria-describedby="inputGroupPrepend1"><%=result.Tel%></a>
                      </div>
                    </div>
                    <div class="col-md-6">
                      <label for="validationCustom03" class="form-label">City</label>
                      <p id="validationCustom03" ><%=result.Address%>,<%=result.state%></p>
                    </div>
                    <div class="col-md-3">
                      <label for="validationCustom05" class="form-label"> Store Keeper</label>
                      <h5 class="font-bold"><%=result.StoreKeeper.firstName.toUpperCase()%> <%=result.StoreKeeper.lastName.toUpperCase()%></h5>
                    </div>
                    <div class="col-md-3">
                     <label for="validationCustom05" class="form-label"> Invoice Ref No</label>
                     <h5 class="font-bold"><%=result.InvoiceNo%></h5>
                   </div>
              </div>
            </div>
          </div>

            <!-- display section -->
            <!--bread crum div -->
<div class="d-flex justify-content-between p-2 m-1 bg-success-subtle ">
  <h5> Invoices</h5>
  <span onclick="location.assign('/api/v1/wHBills/Excel/<%=result._id%>')" title="Export to Xlsx">
    <img height="40" width="40" src="data:image/png;base64,">
  </span>
</div>
<!-- ends here -->
                <%if(whBill.length > 0){%>
                    <%whBill.sort((a,b)=>{return b.createdAt - a.createdAt}).forEach((bill)=>{%>
                        <span onclick="location.assign('/api/v1/bill/<%=bill._id%>')" class="list-group position-relative mb-2" title="refrence no:<%=bill.billReferenceNo%>">
                          <div class="list-group-item list-group-item-action d-flex justify-content-between align-items-center" >
                              <small class="position-absolute top-0  start-50 translate-middle badge rounded-pill bg-warning "title='grand total of this bill'>
                                <%=bill.grandTotal%>
                              </small>
                              
                                 <h6>
                                  <a class="<%if(bill.billStatus === 'Cancelled'){%>
                                    text-danger 
                                    <%}%>" href="/api/v1/bill/<%=bill._id%>"><%=bill.billStatus.substring(0,3).toUpperCase()%>/<%=bill.billReferenceNo%></a>
                                 </h6>
                                 <!-- product name -->
                                <p><%if(bill.status === 'Locked'){%>
                                  <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-file-lock" viewBox="0 0 16 16">
                                    <path d="M8 5a1 1 0 0 1 1 1v1H7V6a1 1 0 0 1 1-1zm2 2.076V6a2 2 0 1 0-4 0v1.076c-.54.166-1 .597-1 1.224v2.4c0 .816.781 1.3 1.5 1.3h3c.719 0 1.5-.484 1.5-1.3V8.3c0-.627-.46-1.058-1-1.224zM6.105 8.125A.637.637 0 0 1 6.5 8h3a.64.64 0 0 1 .395.125c.085.068.105.133.105.175v2.4c0 .042-.02.107-.105.175A.637.637 0 0 1 9.5 11h-3a.637.637 0 0 1-.395-.125C6.02 10.807 6 10.742 6 10.7V8.3c0-.042.02-.107.105-.175z"/>
                                    <path d="M4 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h8a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2H4zm0 1h8a1 1 0 0 1 1 1v12a1 1 0 0 1-1 1H4a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1z"/>
                                  </svg>
                                  <%}else if(bill.status === 'Approved'){%>
                                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-check2-square" viewBox="0 0 16 16">
                                      <path d="M3 14.5A1.5 1.5 0 0 1 1.5 13V3A1.5 1.5 0 0 1 3 1.5h8a.5.5 0 0 1 0 1H3a.5.5 0 0 0-.5.5v10a.5.5 0 0 0 .5.5h10a.5.5 0 0 0 .5-.5V8a.5.5 0 0 1 1 0v5a1.5 1.5 0 0 1-1.5 1.5H3z"/>
                                      <path d="m8.354 10.354 7-7a.5.5 0 0 0-.708-.708L8 9.293 5.354 6.646a.5.5 0 1 0-.708.708l3 3a.5.5 0 0 0 .708 0z"/>
                                    </svg>
                                    <%}%>
                                </p>
                                <p  class="badge s bg-primary <%if(bill.billStatus === 'Cancelled'){%>
                                  bg-danger 
                                  <%}else{%>
                                    bg-success
                                    <%}%>
                                    "><%=bill.billStatus.charAt(0).toUpperCase()%>
                                </p>
                            </div> 
                          </span>
                    <%})%>
                <%}else{%>
                  <p>No Bill to show</p>
                  <%}%>
            </div> 
        </div>
   
   
    <!-- The Modal -->
    <div id="myModal" class="modal flex items-center flex-col justify-center gap-3">
        <!-- Modal content -->
        <div class="modal-content ">
            <div  class="p-3 flex gap-4 flex-col shadow-bg rounded" style="background-color: rgba(255, 255, 255, .15);  
            backdrop-filter: blur(5px);
            -webkit-backdrop-filter: blur(5px); color:black">
                <span onclick="document.getElementById('myModal').style.display='none'" class="w3-button w3-display-topright border w-fit p-2 shadow">&times;</span>
                <h1>Please Ensure to confirm the actual number of product before posting to Storage</h1>
                <label for="toPerform">Mark All Done
                    <input type="checkbox" name="toPerform" >
                </label>
                <input type="number" placeholder="Enter quantity to perform"
                 id="toPerform" required 
                 autocomplete="off">
                 <i>**Note that Qty to perform can not be greater than Qty sent</i>
                <button>Done</button>
            </div>
        </div>
    </div>
</body>
