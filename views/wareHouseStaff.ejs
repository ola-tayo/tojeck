<%-include('./partials/head.ejs')%>

<nav class="navbar bg-black">
  <div class="container-fluid">
    <span class="navbar-brand" onclick="history.back()" href="/" title="Back">
      <img 
      width="30"
      hegith="30"
      src="data:image/png;base64,">
     </span>
    <ul class="navbar-nav me-auto mb-2 mb-lg-0">
      <li class="nav-item dropdown">
        <h5>
          <a
            class="nav-link breadcrumb-item active"
            aria-current="page"
            role="button"
            data-bs-toggle="dropdown"
            aria-expanded="false"
            style="color: #fff !important"
          >
            Employee
          </a>
        </h5>
      </li>
    </ul>
    <%-include('./partials/USER.ejs')%>
    <%-include('./partials/inactive.ejs')%>
  </div>
</nav>
<div class="d-flex justify-content-between p-2 m-1 bg-success-subtle ">
  <h5>Ware House Employee</h5>
  <!-- <span onclick="location.assign('')" title="Download in Xlsx">
    <img height="40" width="40" src="data:image/png;base64,">
  </span> -->
</div>
  <div class="container overflow-hidden flex-wrap d-flex">
    
    <%if(Employee.length > 0){%>
        <% Employee.forEach((Employee)=> { %>
        <div class="card m-1 mb-3 shadow-sm" style="width:13rem;" title="<%=Employee.Username%>" >
            <div class="card-img-top justify-content-center d-flex p-2">
                <img src="<%if(Employee.Image){%>
                    <%= Employee.Image %>
                    <%}else{%>
                        https://cdn.pixabay.com/photo/2019/08/11/18/59/icon-4399701_640.png
                    <%}%>
                    " alt="No Photo uploaded"  width=100 height="100">
            </div>
        
            <div class="card-body" style="overflow:hidden;
            white-space:nowrap;
            text-overflow:ellipsis;
            max-width:150px;">
                <h5 class="card-title"><%= Employee.firstName.toUpperCase() %></h5> 
                <a href="tel:+234<%=Employee.telephone%>">Tel: <%=Employee.telephone%></a>
              </div>
            <a href='/api/v1/employe/<%=Employee._id%>' class="btn btn-dark">Details..</a>
        </div>
        <% }) %>
    <%}else{%>
        <h2>No Employee to display</h2>
        <%}%>
</div>
</div>